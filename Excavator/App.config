<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <!-- Uncomment the following line to look in the current directory -->
    <!-- <add key="ExtensionPath" value="." /> -->
    <add key="ExtensionPath" value="../Extensions" />

    <!-- Don't change this unless you moved the embedded resources -->
    <add key="EmbeddedResources" value="Excavator.Packages" />
  </appSettings>
  <connectionStrings>
    <!-- Holds the Rock connection string (eventually this will be in-app) -->
    <add name="RockContext" providerName="System.Data.SqlClient"
         connectionString="Data Source=localhost; Initial Catalog=RockChMS; User Id=RockUser; password=rockaccess; MultipleActiveResultSets=true"  />
  </connectionStrings>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>  
</configuration>